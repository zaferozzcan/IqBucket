<%- include("../partials/header.ejs") %>

<%- include("../partials/navbar.ejs") %>


<form style="margin: 150px 0 10px 5px; font-weight: 100;" action="/questions" method="POST">
    <h1>Add New <%= tech %> Question</h1>
    <div class="form-group">
        <input required:true type="text" class="form-control" name="question" placeholder="Your question..">
    </div>
    <div class="form-group">
        <input required:true type="text" class="form-control" name="answer" placeholder="Your answer">
    </div>
    <div class="form-group">
        <input required:true type="text" class="form-control" name="tech" placeholder="technology" value=<%= tech %>>
    </div>
    <div class="form-group">
        <input required:true type="text" class="form-control" name="level" placeholder="question level">
    </div>
    <div class="form-group">
        <input required:true type="text" class="form-control" name="source" placeholder="answer source">
    </div>
    <button type="submit" class="btn btn-warning">Submit</button>
</form>




<%- include("../partials/footer.ejs") %>